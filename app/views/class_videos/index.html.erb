<h1>Clases#index</h1>
<%= notice %>

<table align='center'>
    <thead>
        <tr>
            <th>Name</th>
            <th>Description</th>
            <th>Course_id</th>
            <th colspan='3'></th>
        </tr>
    </thead>
    <tbody>
        <% ClassVideo.all.each do |class_video| %>
            <tr>
                <td><%= class_video.name %></td>
                <td><%= class_video.description %></td>
                <td><%= class_video.course_id %></td>
                <td><%= link_to 'Ver detalles', class_videos_show_path(:id => class_video.id) %></td>
            </tr>
        <% end %>
    </tbody>
</table>

